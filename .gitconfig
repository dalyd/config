[user]
	name = dalyd
	email = david.daly@mongodb.com
[core]
	editor = emacs
	excludesfile = ~/.gitignore_global
[filter "media"]
	required = true
	clean = git media clean %f
	smudge = git media smudge %f
[credential]
	helper = osxkeychain
[merge]
	ff = only
        confictStyle = zdiff3
[diff]
        colorMoved = default
[github]
	user = dalyd
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
